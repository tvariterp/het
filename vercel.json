{
    "version": 2,
    "builds": [
        {
            "src": "src/public/**",
            "use": "@vercel/static"
        },
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/images/(.*)",
            "dest": "/src/public/images/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/src/index.js"
        }
    ]
}